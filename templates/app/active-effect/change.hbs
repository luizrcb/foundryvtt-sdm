<li data-index="{{index}}">
  <div class="key">
    <input class="changes-key" type="text" id="changes.{{index}}.key" name="system.changes.{{index}}.key" value="{{change.key}}" list="key-datalist-{{index}}"
    placeholder="{{localize 'SDM.Effect.ChangeProperty'}}">
    <datalist id="key-datalist-{{index}}">
      {{#each selectableKeys}}
      <option value="{{this.value}}">{{this.label}}</option>
      {{/each}}
    </datalist>
  </div>
  <div class="type">
    {{formInput fields.type name=(concat "system.changes." index ".type") value=change.type choices=changeTypes}}
  </div>
  <div class="value">
    {{formInput fields.value name=(concat "system.changes." index ".value") value=change.value elementType="input"}}
  </div>
  <div class="priority">
    {{formInput fields.priority name=(concat "system.changes." index ".priority") value=change.priority placeholder=defaultPriority}}
  </div>
  <div class="controls">
    <button type="button" class="inline-control icon fa-solid fa-trash" data-action="deleteChange"></button>
  </div>
</li>

